{
  "index": {
    "name": "rtx2",
    "settings" : {
      "number_of_shards" : 1,
      "number_of_replicas" : 1
    }
  },
  "experiment_type": {
    "name": "experiment"
  },
  "stage_type": {
    "name": "stage",
    "mapping": {
      "_parent":    { "type": "experiment"},
      "properties": {
        "id":  { "type": "integer" }
      }
    }
  },
  "analysis_type": {
    "name": "analysis",
    "mapping": {
      "properties": {
        "rtx_run_ids":   { "type": "keyword" },
        "name":          { "type": "keyword" },
        "result":        { "type": "object"  },
        "created":       { "type": "date"    }
      }
    }
  },
  "data_point_type": {
    "name": "data_point",
    "mapping": {
      "_parent":    { "type": "stage"},
      "properties": {
        "exp_run":  { "type": "integer" },
        "knobs":    { "type": "object"  },
        "payload":  { "type": "object"  },
        "created":  { "type": "date"    }
      }
    }
  },
  "target_system_type": {
    "name": "target_system",
    "mapping": {
      "properties": {
        "primary_data_provider": {
          "type": "nested",
          "properties": {
            "type":          { "type": "keyword" },
            "serializer":    { "type": "keyword" }
          }
        },
        "change_provider": {
          "type": "nested",
          "properties": {
            "type":          { "type": "keyword" },
            "serializer":    { "type": "keyword" }
          }
        },
        "name":         { "type": "keyword"  },
        "description":  { "type": "text"     },
        "status":       { "type": "keyword"  },
        "created":      { "type": "date"     }
      }
    }
  }
}