{
  "index": {
    "name": "rtx",
    "settings" : {
      "number_of_shards" : 1,
      "number_of_replicas" : 1
    }
  },
  "experiment_type": {
    "name": "experiment",
    "mapping": {
      "properties": {
        "executionStrategy": {
          "type": "nested",
          "properties": {
            "knobs":                   { "type": "object"  },
            "sample_size":             { "type": "integer" },
            "type":                    { "type": "keyword" },
            "stages_count":            { "type": "integer" }
          }
        },
        "created":                  { "type": "date", "format": "yyyy-MM-dd HH||yyyy-MM-dd HH:mm||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss.SSSSSS"},
        "targetSystemId":           { "type": "keyword"  },
        "variable_to_be_optimized": { "type": "keyword"  }
      }
    }
  },
  "stage_type": {
    "name": "stage",
    "mapping": {
      "_parent":    { "type": "experiment"},
      "properties": {
        "number":     { "type": "integer" },
        "knobs":      { "type": "object"  },
        "created":    { "type": "date", "format": "yyyy-MM-dd HH||yyyy-MM-dd HH:mm||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss.SSSSSS"}
      }
    }
  },
  "analysis_type": {
    "name": "analysis",
    "mapping": {
      "properties": {
        "rtx_run_ids":   { "type": "keyword" },
        "name":          { "type": "keyword" },
        "result":        { "type": "object"  },
        "created":       { "type": "date", "format": "yyyy-MM-dd HH||yyyy-MM-dd HH:mm||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss.SSSSSS"}
      }
    }
  },
  "data_point_type": {
    "name": "data_point",
    "mapping": {
      "_parent":    { "type": "stage"},
      "properties": {
        "payload":  { "type": "object"  },
        "created":  { "type": "date", "format": "yyyy-MM-dd HH||yyyy-MM-dd HH:mm||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss.SSSSSS"}
      }
    }
  },
  "target_system_type": {
    "name": "target_system",
    "mapping": {
      "properties": {
        "primary_data_provider": {
          "type": "nested",
          "properties": {
            "type":          { "type": "keyword" },
            "serializer":    { "type": "keyword" },
            "ignore_first_n_samples":  { "type": "integer" }
          }
        },
        "secondary_data_providers": {
          "type": "nested"
        },
        "change_provider": {
          "type": "nested",
          "properties": {
            "type":          { "type": "keyword" },
            "serializer":    { "type": "keyword" }
          }
        },
        "name":         { "type": "keyword"  },
        "description":  { "type": "text"     },
        "status":       { "type": "keyword"  },
        "created":      { "type": "date", "format": "yyyy-MM-dd HH||yyyy-MM-dd HH:mm||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss.SSSSSS"}
      }
    }
  }
}